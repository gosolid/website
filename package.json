    "smartquotes": "^2.3.2",
    "url-parse": "^1.5.10"
  },
  "resolutions": {
    "**/sharp": "^0.30",
    "**/postcss": "^8",
    "**/postcss-discard-overridden": "^5.1",
    "**/sass-loader": "^13.0",
    "**/css-minimizer-webpack-plugin": "^3",
    "**/cssnano": "^5"
  },
  "browser": {
    "crypto": false
  },
  "devDependencies": {
    "@babel/node": "^7.20.2",
    "gh-pages": "^4.0.0",
    "jsonfile": "^6.1.0",
    "wait-on": "^6.0.1"
  }
}
